<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bookshelf [
<!-- Gengeral -->
<!ELEMENT bookshelf (information, books)>

<!-- Information -->
<!ELEMENT information (description, authors, modification_date)>
<!ELEMENT description (#PCDATA)>
<!ELEMENT modification_date (#PCDATA)>
<!ELEMENT authors (author+)>
<!ELEMENT author (name, indeks)>

<!ELEMENT name (#PCDATA)>
<!ELEMENT indeks (#PCDATA)>

<!-- Books -->
<!ELEMENT books (book+)>

<!-- Book -->
<!ELEMENT book (title, book_authors?, languages, realise_day, number_of_pages, binding, dimensions, price+, printing, age_category, type)>
<!ATTLIST book ISBN ID #REQUIRED>
<!ELEMENT title (#PCDATA)>

<!-- Book author/s -->
